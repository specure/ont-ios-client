{
    "name": "RMBTClient",
    "version": "0.0.1",
    "summary": "RMBTClient libary for iOS, OS X and tvOS.",
    "homepage": "https://github.com/specure/...",
    "license": {
        "type": "TODO: Apache v2"
    },
    "source": {
        "git": "https://github.com/specure/...",
        "tag": "0.0.1"
    },
    "authors": "Specure GmbH",
    "platforms": {
        "ios": "8.4",
        "osx": "10.9",
        "tvos": "9.0"
    },
    "source_files": [
        "Sources/**/*.swift",
        "Sources/RMBTTrafficCounter.{h,m}",
        "Sources/RMBTRAMMonitor.{h,m}",
        "Sources/RMBTCPUMonitor.{h,m}",
        "Sources/GetDNSIP.{h,m}",
        "Sources/NSString+IPAddress.{h,m}",
        "Sources/PingUtil.{h,m}"
    ],
    "osx": {
        "exclude_files": [
            "Sources/RMBTHelpers_iOS.swift",
            "Sources/BasicRequestBuilder_iOS.swift",
            "Sources/BasicRequestBuilder_tvOS.swift",

            "vvv_remove/these/later_vvv",
            "Sources/RMBTConnectivity.swift",
            "Sources/RMBTConnectivityTracker.swift",
            "Sources/ControlServer.swift",
            "Sources/ConnectivityService.swift",
            "Sources/RMBTLocationTracker.swift",
            "Sources/QualityOfServiceTest.swift",
            "Sources/RMBTHistoryResult.swift",
            "Sources/RMBTTestResult.swift",
            "Sources/RMBTTestRunner.swift",
            "Sources/RMBTMapMeasurement.swift",
            "Sources/ConnectivityServiceDelegate.swift",
            "Sources/RMBTMapServer.swift",
            "Sources/QualityOfServiceTestDelegate.swift"
        ]
    },
    "ios": {
        "exclude_files": [
            "Sources/BasicRequestBuilder_OSX.swift",
            "Sources/BasicRequestBuilder_tvOS.swift"
        ]
    },
    "tvos": {
        "exclude_files": [
            "Sources/BasicRequestBuilder_iOS.swift",
            "Sources/BasicRequestBuilder_OSX.swift"
        ]
    },
    "___public_header_files": [
        "Sources/RMBTTrafficCounter.h",
        "Sources/RMBTRAMMonitor.h",
        "Sources/RMBTCPUMonitor.h",
        "Sources/GetDNSIP.h",
        "Sources/NSString+IPAddress.h",
        "Sources/PingUtil.h"
    ],
    "dependencies": {
        "AFNetworking": ["~> 2.5.4"],
        "Alamofire": ["~> 3.4"],
        "AlamofireObjectMapper": ["~> 3.0"],
        "BlocksKit": ["~> 2.2.5"],
        "GCNetworkReachability": ["~> 1.3.2"],
        "CocoaAsyncSocket": [],
        "XCGLogger": ["~> 3.3"]
    },
    "frameworks": [
       "Foundation",
       "CoreLocation",
       "SystemConfiguration"
    ],
    "libraries": [
        "resolv"
    ],
    "requires_arc": true,
    "xcconfig": {
        "SWIFT_INCLUDE_PATHS[sdk=iphoneos*]": "$(SRCROOT)/../../rmbt-ios-client/RMBTClientPrivate/iphoneos",
        "SWIFT_INCLUDE_PATHS[sdk=iphonesimulator*]": "$(SRCROOT)/../../rmbt-ios-client/RMBTClientPrivate/iphonesimulator",
        "SWIFT_INCLUDE_PATHS[sdk=macosx*]": "$(SRCROOT)/../../rmbt-ios-client/RMBTClientPrivate/macosx"
    },
    "preserve_paths": [ "RMBTClientPrivate/*" ],
    "prepare_command": "RMBTClientPrivate/injectXcodePath.sh"
}
